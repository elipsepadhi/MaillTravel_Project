<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="false" thread-count="10">
    <listeners >
        <listener class-name="frameWorkOfMailTravel.utilities.listeners.Listeners"/>
        <listener class-name="frameWorkOfMailTravel.utilities.extentReports.ExternalReport"/>
    </listeners>
    <test verbose="2" preserve-order="true" name="CrossBrowserTesting">
        <parameter name="Browser" value="chrome"/>
        <classes>
            <class name="testsOfMailTravel.mailTravelTestsWithCrossBrowserTesting"></class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="CrossBrowserTest">
        <parameter name="Browser" value="edge"/>
        <classes>
            <class name="testsOfMailTravel.mailTravelTestsWithCrossBrowserTesting"></class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="CrossBrowser">
        <parameter name="Browser" value="firefox"/>
        <classes>
            <class name="testsOfMailTravel.mailTravelTestsWithCrossBrowserTesting"></class>
        </classes>
    </test>
</suite>